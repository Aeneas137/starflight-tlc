    <PlanetSurfaceEvents>
      <Event>function AfrywiogPrisonOnEvent()  
    if (event == 0) then  
        x,y = L_GetOffsetPosition()
        x2,y2 = L_GetPlayerTVPosition();
        if (CheckDistance(x,y,x2,y2) &lt; 150)  then 
            Q_LoadQuestbyID(13);
            if (not(Q_CheckRequirement(0))) then
                if (L_CheckInventoryFor( 186, 60)) then
                    L_PostMessage(255, 0, 0,"You delivered the Prisoners. Return to the Starport for debriefing.");  
                    L_Interacted(13); 
                    L_RemoveItemFromInventory( 186, 60);
                else
                    L_PostMessage(0, 0, 255,"You don't have the 60 Prisoners you are suppose to deliver!");  
                end
            else
                L_PostMessage(0, 0, 255,"You have already delivered the 60 Prisoners!");  
            end
        else
             L_PostMessage(0, 0, 255,"You aren't close enough to deliver anything!");  
        end
    end 
end



function AfrywiogPrisonGetActions()  
    scanned = L_IsScanned();
    if (scanned) then 
        L_SetActions("Deliver"); 
    end 
end</Event>
